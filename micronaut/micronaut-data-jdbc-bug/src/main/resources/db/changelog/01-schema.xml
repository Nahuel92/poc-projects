<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
    <changeSet id="01" author="nahuel92">
        <createTable tableName="my_table">
            <column name="id" type="int" autoIncrement="true"/>
            <column name="attribute" type="varchar2"/>
            <column name="another_attribute" type="varchar2"/>
        </createTable>
    </changeSet>

    <changeSet id="02" author="nahuel92">
        <insert tableName="my_table">
            <column name="attribute" value="anything"/>
            <column name="another_attribute" value="c"/>
        </insert>
        <insert tableName="my_table">
            <column name="attribute" value="anything 2"/>
            <column name="another_attribute" value="u"/>
        </insert>
        <insert tableName="my_table">
            <column name="attribute" value="anything 3"/>
            <column name="another_attribute" value="d"/>
        </insert>
    </changeSet>
</databaseChangeLog>